<launch>
  <node pkg="tf"
        type="static_transform_publisher"
        name="link1_broadcaster"
        args="0 0 0 0 0 0 map base_link 50" />

  <node name="movie_publisher_node" pkg="movie_publisher" type="movie_publisher_node">
	<param name="movie_file" type="string" value="$(dirname)/../../runs/test1.mp4" />
	<param name="fps" type="double" value="30" />
  </node>

  <remap from="/camera/image_raw" to="/movie"/>

  <node name="Mono" pkg="orb_slam3" type="Mono" output="screen" 
        args="$(dirname)/../../../non_ros/orb_slam3/Vocabulary/ORBvoc.txt $(dirname)/orb_slam3.yaml" />
</launch>